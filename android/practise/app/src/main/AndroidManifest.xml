<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poppo.practise">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".instaclone.utils.MasterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".instaclone.activity.PostListActivity"></activity>
        <activity android:name=".instaclone.activity.LoginActivity" />
        <activity android:name=".instaclone.activity.EmailSignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>